<p id="notice"><%= notice %></p>

<h1>Scores</h1>

<table>
  <thead>
    <tr>
      <th>Set</th>
      <th>Score</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scores.each do |score| %>
      <tr>
        <td><%= score.set %></td>
        <td><%= score.score %></td>
        <td><%= score.team.name %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Score', new_match_score_path %>
